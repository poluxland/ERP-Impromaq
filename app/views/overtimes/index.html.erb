<p id="notice"><%= notice %></p>

<h1>Overtimes</h1>

<table>
  <thead>
    <tr>
      <th>Inicio</th>
      <th>Termino</th>
      <th>Personal</th>
      <th>Cantidad</th>
      <th>Motivo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @overtimes.each do |overtime| %>
      <tr>
        <td><%= overtime.inicio %></td>
        <td><%= overtime.termino %></td>
        <td><%= overtime.personal_id %></td>
        <td><%= overtime.cantidad %></td>
        <td><%= overtime.motivo %></td>
        <td><%= link_to 'Show', overtime %></td>
        <td><%= link_to 'Edit', edit_overtime_path(overtime) %></td>
        <td><%= link_to 'Destroy', overtime, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Overtime', new_overtime_path %>
